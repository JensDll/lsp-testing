{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "allowJs": true,
    "checkJs": false,
    "lib": ["ESNext"],
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
